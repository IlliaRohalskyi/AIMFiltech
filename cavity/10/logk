FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "10";
    object      logk;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform -6.90776;

boundaryField
{
    movingWall
    {
        type            calculated;
        value           uniform -6.90776;
    }
    fixedWalls
    {
        type            calculated;
        value           uniform -6.90776;
    }
    frontAndBack
    {
        type            empty;
    }
}


